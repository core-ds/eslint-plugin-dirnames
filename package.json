{
  "name": "eslint-plugin-dirnames",
  "version": "1.0.3",
  "description": "Eslint rule for consistent dirnames",
  "author": "core-ds contributors",
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "test": "jest .",
    "coverage": "jest . --coverage",
    "release-patch": "npm version patch -m 'chore(*): patch version'",
    "release-minor": "npm version minor -m 'chore(*): minor version'",
    "release-major": "npm version major -m 'chore(*): major version'",
    "postversion": "git push origin master && git push --tags && npm run pub",
    "pub": "npm publish"
  },
  "peerDependencies": {
    "kebab-case": "1.0.0"
  },
  "dependencies": {
    "kebab-case": "1.0.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "jest": "^25.2.7"
  },
  "packageManager": "yarn@4.1.1"
}
